{
  "headers": [
    {
      "source": "/(.*)\\.(ttf|otf|eot|woff|woff2)$",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, public"
        }
      ]
    },
    {
      "source": "/(.*)\\.js$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, public"
        }
      ]
    },
    {
      "source": "/(.*)\\.css$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, public"
        }
      ]
    },
    {
      "source": "/(.*)\\.(jpeg|jpg|png|gif|svg)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, public"
        }
      ]
    }
  ]
}
